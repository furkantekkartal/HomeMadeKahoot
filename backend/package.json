{
  "name": "homemadekahoot-backend",
  "version": "1.0.0",
  "description": "Backend server for HomeMadeKahoot - English Learning Game Platform",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "start:local": "cross-env NODE_ENV=local PORT=5010 node server.js",
    "start:dev": "cross-env NODE_ENV=development PORT=5020 node server.js",
    "start:prod": "cross-env NODE_ENV=production PORT=5030 node server.js",
    "dev:local": "cross-env NODE_ENV=local PORT=5010 nodemon server.js",
    "dev:dev": "cross-env NODE_ENV=development PORT=5020 nodemon server.js",
    "dev:prod": "cross-env NODE_ENV=production PORT=5030 nodemon server.js",
    "reset-db": "node scripts/resetDatabase.js",
    "db-status": "node scripts/databaseStatus.js",
    "db-cleanup": "node scripts/safeCleanup.js",
    "import-words": "node scripts/importWords.js",
    "migrate-to-prod": "node scripts/migrateDataToProduction.js",
    "reset-prod-db": "node scripts/resetProductionDatabase.js",
    "reset-game-performance": "node scripts/resetGamePerformance.js",
    "generate-all-images": "node scripts/generateAllWordImages.js",
    "reset-and-import": "node scripts/resetAndImportDatabase.js",
    "sync-images-to-prod": "node scripts/syncImagesToProduction.js",
    "clone-dev-to-prod": "node scripts/cloneDevToProduction.js",
    "clone-users-prod-to-dev": "node scripts/cloneUsersProdToDev.js"
  },
  "keywords": [
    "quiz",
    "english-learning",
    "kahoot",
    "education"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "microsoft-cognitiveservices-speech-sdk": "^1.34.1",
    "mongoose": "^7.5.0",
    "multer": "^2.0.2",
    "pdf-parse": "^1.1.1",
    "socket.io": "^4.6.1",
    "youtube-transcript": "^1.2.1",
    "ytdl-core": "^4.11.5"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.4.4",
    "nodemon": "^3.0.1"
  }
}
